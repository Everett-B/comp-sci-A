                            enemyHealth = enemyHealth - dam;                            
                            if (enemyHealth > 100){ //put health and shield back seperately
                                enemy1.setHealth(100);
                                enemy1.setSheild(enemyHealth - 100);               
                            } else {
                                enemy1.setHealth(enemyHealth);
                                enemy1.setSheild(0);
                            }
