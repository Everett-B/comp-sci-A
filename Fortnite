                System.out.println("you see an enemy and engage in battle\nYou can either heal or attack but you can only use your healing once.\ntype heal to heal, and attack to attack");
                
                String input2 = scan.nextLine();
                //attack statement, use damage from weapon to do player and random for enemy
                if (input2.equalsIgnoreCase("Attack")) {
                            // change damage to variable and add health and sheild into one variable
                            double dam = player.getDamage();
                            double enemyHealth = enemy4.getHealth() + enemy4.getSheild();
                            enemyHealth = enemyHealth - dam;                            
                            if (enemyHealth > 100){ //put health and shield back seperately
                                enemy4.setHealth(100);
                                enemy4.setSheild(enemyHealth - 100);               
                            } else {
                                enemy4.setHealth(enemyHealth);
                                enemy4.setSheild(0);
                            }
                            System.out.println("Damage done: " + dam);
                            double playerHealth = player.getHealth() + player.getSheild();
                            //random damage for enemy
                            playerHealth = playerHealth - r.nextInt(40);                            
                            if (playerHealth > 100){
                                player.setHealth(100);
                                player.setSheild(playerHealth - 100);               
                            } else {
                                player.setHealth(playerHealth);
                                 player.setSheild(0);
                            }
                            
                            
                          
                    //heal if statement, change health by using heal and enemy healing is random
                        } else if (input2.equalsIgnoreCase("Heal")) {
                            enemy4.setHealth(enemy4.getHealth() + r.nextInt(50));
                            enemy4.setSheild(enemy4.getSheild() + r.nextInt(50));
                            player.setHealth(player.getHealth() + player.getHealing());
                            player.setSheild(player.getSheild() + player.getSheilding());
                            player.addHeal(none);
                            // dont allow for over healing
                            if (player.getHealth() > 100){
                                player.setHealth(100);                                           
                            }  
                             if (player.getSheild() > 100){
                                player.setSheild(100);                                           
                            }  
                             if (enemy4.getHealth() > 100){
                                enemy4.setHealth(100);                                           
                            }  
                             if (enemy4.getSheild() > 100){
                                enemy4.setSheild(100);                                           
                            }  
                        } 
                //status update for player 
                System.out.println("YOU:" +  player.toString() + "ENEMY" + enemy4.toString());
            }
            //defeat and win end battle statements
            if (player.getHealth() <= 0){
                    System.out.println("You were DEFEATED!! :( ");
            } else if (enemy4.getHealth() <= 0){
                    System.out.println("You WON!! :) ");
                    System.out.println("you survived the fourth circle and it is now closing, you loot up a little more");
