                    } else if (rWe > 1 && rWe <= 5){  
                         com.mycompany.fortnite.Weapon scar = new com.mycompany.fortnite.Weapon( "", "Scar", 40, 25);
                        System.out.println("You Found: \n" + scar.toString() + "\nWould you like to pick up this weapon and get rid of your old one:\n" + player.getInventory() + "\nPress 1 to pick it up and anything else to keep your current weapon.");
                         String input3 = scan.nextLine();
                        if (input3.equalsIgnoreCase("1"))
                        player.addWeapon(scar);
                    } else if (rWe > 5 && rWe <= 15){
                        com.mycompany.fortnite.Weapon pNinety = new com.mycompany.fortnite.Weapon("", "P90", 22, 20);
                        System.out.println("You Found: \n" + pNinety.toString() + "\nWould you like to pick up this weapon and get rid of your old one:\n" + player.getInventory() + "\nPress 1 to pick it up and anything else to keep your current weapon.");
                         String input3 = scan.nextLine();
                        if (input3.equalsIgnoreCase("1"))
                        player.addWeapon(pNinety);
                    } else if (rWe > 15 && rWe <= 35){   
                         com.mycompany.fortnite.Weapon sniper = new com.mycompany.fortnite.Weapon("", "Sniper", 200, 2);
                        System.out.println("You Found: \n" + sniper.toString() + "\nWould you like to pick up this weapon and get rid of your old one:\n" + player.getInventory() + "\nPress 1 to pick it up and anything else to keep your current weapon.");
                         String input3 = scan.nextLine();
                        if (input3.equalsIgnoreCase("1"))
                         player.addWeapon(sniper);
                    } else if (rWe > 35 && rWe <= 65) {  
                         com.mycompany.fortnite.Weapon pumpShotgun = new com.mycompany.fortnite.Weapon("", "Pump Shotgun", 150, 10);
                        System.out.println("You Found: \n" + pumpShotgun.toString() + "\nWould you like to pick up this weapon and get rid of your old one:\n" + player.getInventory() + "\nPress 1 to pick it up and anything else to keep your current weapon.");
                         String input3 = scan.nextLine();
                        if (input3.equalsIgnoreCase("1"))
                        player.addWeapon(pumpShotgun);
                    } else if (rWe > 65 && rWe <= 100) {
                         com.mycompany.fortnite.Weapon smg = new com.mycompany.fortnite.Weapon("", "Smg", 20, 15);
                        System.out.println("You Found: \n" + smg.toString() + "\nWould you like to pick up this weapon and get rid of your old one:\n" + player.getInventory() + "\nPress 1 to pick it up and anything else to keep your current weapon.");
                         String input3 = scan.nextLine();
                        if (input3.equalsIgnoreCase("1"))
                        player.addWeapon(smg);
                    }
                    System.out.println("you find a new healing item and use your old one to make room for it");
                    rWe = r.nextInt(100)+1;
                    if (rWe <= 1){
                         com.mycompany.fortnite.Heal chug = new com.mycompany.fortnite.Heal("LEGENDARY","Chug Jug", 100, 100);
                        player.addHeal(chug);
                       player.setHealth(player.getHealth() + player.getHealing());
                            player.setSheild(player.getSheild() + player.getSheilding());
                            player.addHeal(none);
                            if (player.getHealth() > 100){
                                player.setHealth(100);                                           
                            }  
                             if (player.getSheild() > 100){
                                player.setSheild(100);                                           
                            }  
                             
                        System.out.println(chug.toString());
                        
                    } else if (rWe > 1 && rWe <= 5){
                        com.mycompany.fortnite.Heal slurp = new com.mycompany.fortnite.Heal("epic","Slurp Juice", 0, 75);
                        player.addHeal(slurp);
                        player.setHealth(player.getHealth() + player.getHealing());
                            player.setSheild(player.getSheild() + player.getSheilding());
                            player.addHeal(none);
                            if (player.getHealth() > 100){
                                player.setHealth(100);                                           
                            }  
                             if (player.getSheild() > 100){
                                player.setSheild(100);                                           
                            }  
                            
                        System.out.println(slurp.toString());
                        
                    } else if (rWe > 5 && rWe <= 15){
                         com.mycompany.fortnite.Heal pot = new com.mycompany.fortnite.Heal("rare","Big Pot", 0, 50);
                        player.addHeal(pot);
                        player.setHealth(player.getHealth() + player.getHealing());
                            player.setSheild(player.getSheild() + player.getSheilding());
                            player.addHeal(none);
                            if (player.getHealth() > 100){
                                player.setHealth(100);                                           
                            }  
                             if (player.getSheild() > 100){
                                player.setSheild(100);                                           
                            }  
                             
                        System.out.println(pot.toString());
                        
                    } else if (rWe > 15 && rWe <= 35){
                         com.mycompany.fortnite.Heal meddy = new com.mycompany.fortnite.Heal("uncommon","Medkit", 100, 0);
                        player.addHeal(meddy);
                        player.setHealth(player.getHealth() + player.getHealing());
                            player.setSheild(player.getSheild() + player.getSheilding());
                            player.addHeal(none);
                            if (player.getHealth() > 100){
                                player.setHealth(100);                                           
                            }  
                             if (player.getSheild() > 100){
                                player.setSheild(100);                                           
                            }  
                            
                        System.out.println(meddy.toString());
                        
                    } else if (rWe > 35 && rWe <= 65) {
                        com.mycompany.fortnite.Heal mini = new com.mycompany.fortnite.Heal("uncommon","Mini", 0, 25);
                        player.addHeal(mini);
                        player.setHealth(player.getHealth() + player.getHealing());
                            player.setSheild(player.getSheild() + player.getSheilding());
                            player.addHeal(none);
                            if (player.getHealth() > 100){
                                player.setHealth(100);                                           
                            }  
                             if (player.getSheild() > 100){
                                player.setSheild(100);                                           
                            }  
                             
                        System.out.println(mini.toString());
                        
                    } else if (rWe > 65 && rWe <= 100) {
                         com.mycompany.fortnite.Heal bandage = new com.mycompany.fortnite.Heal("common","Bandage", 15, 0);
                        player.addHeal(bandage);
                        player.setHealth(player.getHealth() + player.getHealing());
                            player.setSheild(player.getSheild() + player.getSheilding());
                            player.addHeal(none);
                            if (player.getHealth() > 100){
                                player.setHealth(100);                                           
                            }  
                             if (player.getSheild() > 100){
                                player.setSheild(100);                                           
                            }  
                            
                        System.out.println(bandage.toString());
                    }
                     Character enemy7 = new com.mycompany.fortnite.Character(rWe = r.nextInt(100)+1, rWe = r.nextInt(100)+1);
            //while for repeating until someone dies
            while (player.getHealth() > 0 && enemy7.getHealth() > 0){
                System.out.println("you see an enemy and engage in battle\nYou can either heal or attack but you can only use your healing once.\ntype heal to heal, and attack to attack");
                
                String input2 = scan.nextLine();
                //attack statement, use damage from weapon to do player and random for enemy
                if (input2.equalsIgnoreCase("Attack")) {
                            // change damage to variable and add health and sheild into one variable
                            double dam = player.getDamage();
                            double enemyHealth = enemy7.getHealth() + enemy7.getSheild();
                            enemyHealth = enemyHealth - dam;                            
                            if (enemyHealth > 100){ //put health and shield back seperately
                                enemy7.setHealth(100);
                                enemy7.setSheild(enemyHealth - 100);               
                            } else {
                                enemy7.setHealth(enemyHealth);
                                enemy7.setSheild(0);
                            }
                            System.out.println("Damage done: " + dam);
                            double playerHealth = player.getHealth() + player.getSheild();
                            //random damage for enemy
                            playerHealth = playerHealth - r.nextInt(40);                            
                            if (playerHealth > 100){
                                player.setHealth(100);
                                player.setSheild(playerHealth - 100);               
                            } else {
                                player.setHealth(playerHealth);
                                 player.setSheild(0);
                            }
                            
                            
                          
                    //heal if statement, change health by using heal and enemy healing is random
                        } else if (input2.equalsIgnoreCase("Heal")) {
                            enemy7.setHealth(enemy7.getHealth() + r.nextInt(50));
                            enemy7.setSheild(enemy7.getSheild() + r.nextInt(50));
                            player.setHealth(player.getHealth() + player.getHealing());
                            player.setSheild(player.getSheild() + player.getSheilding());
                            player.addHeal(none);
                            // dont allow for over healing
                            if (player.getHealth() > 100){
                                player.setHealth(100);                                           
                            }  
                             if (player.getSheild() > 100){
                                player.setSheild(100);                                           
                            }  
                             if (enemy7.getHealth() > 100){
                                enemy7.setHealth(100);                                           
                            }  
                             if (enemy7.getSheild() > 100){
                                enemy7.setSheild(100);                                           
                            }  
                        } 
                //status update for player 
                System.out.println("YOU:" +  player.toString() + "ENEMY" + enemy7.toString());
            }
            //defeat and win end battle statements
            if (player.getHealth() <= 0){
                    System.out.println("You were DEFEATED!! :( ");
            } else if (enemy7.getHealth() <= 0){
                    System.out.println("""
                                       As you stand there, utop the mound of corpses you left in your brutal wake, the storm slows, halting its 
                                        seemingly endless advance.  The sky opens above you, as the fabled Battle Bus swoops low, 
                                       blaring its horn to celebrate your victory over those other lesser mortals
                                        only you survived, out of the countless adversaries and enemies, you remain, tall and victorius.
                                        Soaking in the sunlight on your skin, you now fully realize what it means to achieve vicotry, 
                                       and how great it feels to get that Vic Roy all for yourself.""");
            }
                }
                }
                }
                }
            }            
        } 
            
    
            
            
            
            
            
            
            
        }
    }
}
//initial class item tfor rarity and name
class Item{
    String name;
    String rarity = "";
    public Item (String inRarity, String inName){
        name = inName;
        rarity = inRarity;
    }   
    public String getRarity(){
        return rarity;
    }
    public void setRarity(String inRarity){
        rarity = inRarity;
    }
    
}
//weapon class extends item and adds damage and accuracy. also methods for different rarity system and damage
class Weapon extends Item{
    double damage = 0;
    int accuracy = 0;
    
   String[] rars = {"common", "uncommon", "rare", "epic", "LEGENDARY", "MYTHIC"};
    public Weapon(String inRarity, String inName, double inDamage, int inAccuracy){
            super(inRarity, inName);
             Random r = new Random();
             rarity = rars[r.nextInt(4)];
            double i = 0;
        if (rarity.equalsIgnoreCase("common")){
        i = 0.75;
    } else if (rarity.equalsIgnoreCase("uncommon")){
        i = 0.9;
    } else if (rarity.equalsIgnoreCase("rare")){
        i = 1;
    } else if (rarity.equalsIgnoreCase("epic")){
        i = 1.1;    
    } else if (rarity.equalsIgnoreCase("legendary")){
        i = 1.2 ; 
    } else if (rarity.equalsIgnoreCase("mythic")){
        i = 1.25;    
    }
            damage = inDamage * i;
            accuracy = inAccuracy;
                    }
    public double getDamage(){
        Random r = new Random();
        return Math.round(r.nextDouble(damage) + accuracy);
    }
    public void setDamage(int inDamage){
        damage = inDamage;
    }
     public int getAccuracy(){
        return accuracy;
    }
    public void setAccuracy(int inAccuracy){
        accuracy = inAccuracy;
    }
    public String toString(){
        String i = "----------------" + "\nWeapon: " + name + "\nRarity: " + rarity + "\nDamage: " + damage + "\nAccuracy: " + accuracy;
        return i;
    }
}
//ectends item, addas healing and sheild methods and variables
class Heal extends Item{
    int healing; 
    int shield;   
    public Heal(String inRarity, String inName, int inHealing, int inShield){
            super(inRarity, inName);
            healing = inHealing;
            shield = inShield;
                    }
      public int getHealing(){
        return healing;
    }
    public void setHealing(int inHealing){
        healing = inHealing;
    }
      public int getShield(){
        return shield;
    }
    public void setShield(int inShield){
        shield = inShield;
    }
    public String toString(){
        String i = "----------------" + "\nHeal: " + name + "\nRarity: " + rarity + "\nHealing: " + healing + "\nShield: " + shield;
        return i;
    }
}
//base calss with just health and sheild.
class Character{
    double health = 100;
    double sheild = 0;
    public Character(double inHealth, double inSheild){
        health = inHealth;
        sheild = inSheild;
    }
      public double getHealth(){
        return health;
    }
    public void setHealth(double inHealth){
        health = inHealth;
    }
    public double getSheild(){
        return sheild;
    }
    public void setSheild(double inSheild){
        sheild = inSheild;
    }
    public String toString(){
        return "----------------" + "\nHealth: " + health + "\nSheild: " + sheild + "\n";
    }
}
//player class extends character with new inventory methods for weapons and heals
class Player extends Character {
    Weapon[] invWeapon; 
    Heal[] invHeal;
    public Player(double inHealth, double inSheild){
        super(inHealth, inSheild);
    }
    public void addWeapon(Weapon newWeapon){
        invWeapon = new Weapon[1];
        invWeapon[0] = newWeapon;
    }
    public String getInventory(){
        return invWeapon[0].toString() + "\n" + invHeal[0].toString();
    }
    public double getDamage(){
        return invWeapon[0].getDamage();
    }
    public int getAccuracy(){
        return invWeapon[0].accuracy;
    }
    public void addHeal(Heal newHeal){
        invHeal = new Heal[1];
        invHeal[0] = newHeal;
    }
    public int getHealing(){
        return invHeal[0].healing;
    }
    public int getSheilding(){
        return invHeal[0].shield;
    }    
}    
    
    
  
